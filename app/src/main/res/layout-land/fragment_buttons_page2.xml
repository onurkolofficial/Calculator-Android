<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.calculator.ButtonsPage2Fragment">
    <!-- Scroll Page 2 !-->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <!-- Button Line 1 !-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <!-- Log Button !-->
                <Button
                    android:id="@+id/logButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:text="LOG"
                    android:textSize="18sp" />
                <!-- Cot Button !-->
                <Button
                    android:id="@+id/cotButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_toEndOf="@id/logButton"
                    android:text="COT"
                    android:textSize="18sp" />
                <!-- Deg Button !-->
                <Button
                    android:id="@+id/degButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_toEndOf="@id/cotButton"
                    android:text="DEG"
                    android:textSize="18sp" />
                <!-- Rad Button !-->
                <Button
                    android:id="@+id/radButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_alignParentEnd="true"
                    android:layout_toEndOf="@id/degButton"
                    android:textSize="18sp"
                    android:text="RAD" />
            </RelativeLayout>
        </LinearLayout>
        <!-- Button Line 2 !-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <!-- Abs Button !-->
                <Button
                    android:id="@+id/absButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:text="ABS"
                    android:textSize="18sp" />
                <!-- Sin Button !-->
                <Button
                    android:id="@+id/sinButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_toEndOf="@id/absButton"
                    android:text="SIN"
                    android:textSize="18sp" />
                <!-- Cos Button !-->
                <Button
                    android:id="@+id/cosButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_toEndOf="@id/sinButton"
                    android:text="COS"
                    android:textSize="18sp" />
                <!-- Tan Button !-->
                <Button
                    android:id="@+id/tanButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_alignParentEnd="true"
                    android:layout_toEndOf="@id/cosButton"
                    android:textSize="18sp"
                    android:text="TAN" />
            </RelativeLayout>
        </LinearLayout>
        <!-- Button Line 3 !-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <!-- Pi Button !-->
                <Button
                    android:id="@+id/piButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:text="&#960;"
                    android:textAllCaps="false"
                    android:textSize="18sp" />
                <!-- E Button !-->
                <Button
                    android:id="@+id/eButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_toEndOf="@id/piButton"
                    android:text="e"
                    android:textAllCaps="false"
                    android:textSize="18sp" />
                <!-- Fact Button !-->
                <Button
                    android:id="@+id/factButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_toEndOf="@id/eButton"
                    android:text="n!"
                    android:textAllCaps="false"
                    android:textSize="18sp" />
                <!-- Power Button !-->
                <Button
                    android:id="@+id/powerButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="82dp"
                    android:layout_height="100dp"
                    android:layout_alignParentEnd="true"
                    android:layout_toEndOf="@id/factButton"
                    android:textSize="18sp"
                    android:textAllCaps="false"
                    android:text="x&#696;" />
            </RelativeLayout>
        </LinearLayout>
        <!-- Button Line 4 !-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <!-- Sqrt Button !-->
                <Button
                    android:id="@+id/sqrtButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:text="&#8730;"
                    android:textSize="18sp" />
                <!-- Floor Button !-->
                <Button
                    android:id="@+id/lnButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_toEndOf="@id/sqrtButton"
                    android:text="LN"
                    android:textSize="18sp" />
                <!-- Bracket On Button !-->
                <Button
                    android:id="@+id/brackOnButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_toEndOf="@id/lnButton"
                    android:text="("
                    android:textSize="18sp" />
                <!-- Bracket On Button !-->
                <Button
                    android:id="@+id/brackOffButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_alignParentEnd="true"
                    android:layout_toEndOf="@id/brackOnButton"
                    android:textSize="18sp"
                    android:text=")" />

            </RelativeLayout>
        </LinearLayout>

        <!-- Button Line 5 !-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <!-- Sinh Button !-->
                <Button
                    android:id="@+id/sinhButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:text="SINH"
                    android:textSize="18sp" />
                <!-- ASin Button !-->
                <Button
                    android:id="@+id/asinButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_toEndOf="@id/sinhButton"
                    android:text="ASIN"
                    android:textSize="18sp" />
                <!-- ACos Button !-->
                <Button
                    android:id="@+id/acosButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_toEndOf="@id/asinButton"
                    android:text="ACOS"
                    android:textSize="18sp" />
                <!-- Tan Button !-->
                <Button
                    android:id="@+id/atanButton"
                    style="@style/Widget.AppCompat.Button.Borderless"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_alignParentEnd="true"
                    android:layout_toEndOf="@id/acosButton"
                    android:textSize="18sp"
                    android:text="ATAN" />
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>